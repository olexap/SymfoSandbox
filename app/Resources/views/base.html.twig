<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />

        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.0/css/bootstrap.no-icons.min.css">
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/2.0/css/font-awesome.css">

        {% block stylesheets %}{% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>
    <body>

    {{ knp_menu_render('OlexaTestBundle:Navigation:mainMenu', {'currentClass': 'active', 'template': 'OlexaTestBundle:Navigation:knp_menu.html.twig'}) }}
    {{ knp_menu_render('OlexaTestBundle:Navigation:userMenu', {'currentClass': 'active', 'template': 'OlexaTestBundle:Navigation:knp_menu.html.twig'}) }}



    {% block body %}{% endblock %}

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
        <script type="text/javascript" src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.0/js/bootstrap.min.js"></script>

    {% block javascripts %}{% endblock %}
    </body>
</html>
